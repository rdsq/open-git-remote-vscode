{
  "name": "literal-open-git-remote",
  "displayName": "Open Git Remote",
  "description": "Exactly what it says - just open a git repository's remote in the browser",
  "version": "1.1.0",
  "publisher": "rdsq",
  "icon": "./icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdsq/open-git-remote-vscode"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "literal-open-git-remote.open-git-remote",
        "title": "Open Git Remote"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
